<div class="container">
    <br>
    <center><h3>All Expenses</h3></center>
    <br>
     <table class="table">
            <thead>
                <tr>
                    <th scope="col">Description</th>
                    <th scope="col">Category</th>
                     <th scope="col">Totals</th>
                </tr>
            </thead>
            <tbody>
                {{#each expenses}}
                    <tr>
                        <td>{{this.expense}} </td>
                        <td>{{this.category_type}}</td>
                        <td>{{this.total}}</td>
                    </tr>
                {{/each}}

            </tbody>
        </table>
</div>